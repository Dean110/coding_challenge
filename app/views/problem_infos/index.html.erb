<p id="notice"><%= notice %></p>

<h1>Problem Infos</h1>

<table>
  <thead>
    <tr>
      <th>Key</th>
      <th>Answer</th>
      <th>Problem ID</th>
      <th>User ID</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @problem_infos.each do |problem_info| %>
      <tr>
        <td><%= problem_info.key %></td>
        <td><%= problem_info.answer %></td>
        <td><%= problem_info.problem_id%></td>
        <td><%= problem_info.user_id %></td>
        <td><%= link_to 'Show', problem_info %></td>
        <td><%= link_to 'Edit', edit_problem_info_path(problem_info) %></td>
        <td><%= link_to 'Destroy', problem_info, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Problem Info', new_problem_info_path %>
